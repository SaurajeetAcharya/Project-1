# Creating a list
my_list = [1, 2, 3, 4, 5]

# Adding elements to the list
my_list.append(6)
print("List after adding element:", my_list)

# Removing elements from the list
my_list.remove(3)
print("List after removing element:", my_list)

# Modifying elements in the list
my_list[0] = 10
print("List after modifying element:", my_list)


# Creating a dictionary
my_dict = {'a': 1, 'b': 2, 'c': 3}

# Adding elements to the dictionary
my_dict['d'] = 4
print("Dictionary after adding element:", my_dict)

# Removing elements from the dictionary
del my_dict['b']
print("Dictionary after removing element:", my_dict)

# Modifying elements in the dictionary
my_dict['a'] = 100
print("Dictionary after modifying element:", my_dict)


# Creating a set
my_set = {1, 2, 3, 4, 5}

# Adding elements to the set
my_set.add(6)
print("Set after adding element:", my_set)

# Removing elements from the set
my_set.remove(3)
print("Set after removing element:", my_set)

# Modifying elements in the set (Sets are immutable, so you cannot modify individual elements)
# However, you can create a new set with modified elements
my_set = {x * 2 for x in my_set}
print("Set after modifying elements:", my_set)
